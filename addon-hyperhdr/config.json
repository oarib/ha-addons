{
  "name": "HyperHDR",
  "version": "latest",
  "slug": "hyperhdr",
  "description": "Next-gen Ambilight system for your TV setup with grabber and WLED support.",
  "url": "https://www.hyperhdr.eu/",
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ],
  "startup": "application",
  "apparmor": false,
  "boot": "auto",
  "ports": {
    "8090/tcp": 8090,
    "8092/tcp": null,
    "19333/tcp": 19333,
    "19400/tcp": 19400,
    "19444/tcp": 19444,
    "19445/tcp": 19445
  },
  "ports_description": {
    "8090/tcp": "Web Admin",
    "8092/tcp": "SSL Web Admin",
    "19333/tcp": "Boblight Server",
    "19400/tcp": "Flatbuffers Receiver",
    "19444/tcp": "JSON-RPC",
    "19445/tcp": "Proto Buffers Server"
  },
  "map": ["config:rw", "ssl", "media:rw"],
  "image": "obaida/hyperhdr-{arch}",
  "privileged": ["SYS_ADMIN", "NET_ADMIN", "SYS_RAWIO"],
  "uart": true,
  "usb": true,
  "video": true,
  "gpio": true,
  "devices": [
    "/dev/video0:/dev/video0:rwm",
    "/dev/spidev0.0",
    "/dev/vchiq",
    "/dev/amvideo",
    "/dev/fb0",
    "/dev/mem"
  ],
  "webui": "http://[HOST]:[PORT:8090]",
  "panel_icon": "mdi:led-strip-variant",
  "init": false,
  "options": {},
  "schema": {}
}
